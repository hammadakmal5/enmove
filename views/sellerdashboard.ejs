  <body>
    <!-- navbar -->
    <%- include('partials/assistant-dash-nav'); %>

    <!-- Main Section -->
    <main class="" style="background-color: #f5f5f5bf">
      <!-- Profile card and Active order -->
      <div class="d-flex justify-content-start py-5">
        <div class="card ms-4" style="width: 15rem">
          <div class="card-header">
            <a href="" class="px-2 link-underline-light">
              <img src="images/user.png" alt="" />
              <span class="px-4">username</span>
            </a>
          </div>
          <ul class="list-group list-group-flush">
            <li class="list-group-item border border-1 opacity-50">
              Inbox response rate
              <div
                class="progress"
                role="progressbar"
                aria-label="Example with label"
                aria-valuenow="25"
                aria-valuemin="0"
                aria-valuemax="100"
              >
                <div class="progress-bar" style="width: 25%">0%</div>
              </div>
            </li>
            <li class="list-group-item border border-1 opacity-50">
              Inbox response time
              <div
                class="progress"
                role="progressbar"
                aria-label="Example with label"
                aria-valuenow="25"
                aria-valuemin="0"
                aria-valuemax="100"
              >
                <div class="progress-bar" style="width: 100%">100%</div>
              </div>
            </li>
            <li class="list-group-item border border-1 opacity-50">
              Bookings completion
              <div
                class="progress"
                role="progressbar"
                aria-label="Example with label"
                aria-valuenow="25"
                aria-valuemin="0"
                aria-valuemax="100"
              >
                <div class="progress-bar" style="width: 50%">0%</div>
              </div>
            </li>

            <li class="list-group-item border border-1 opacity-50">
              Earned in Month <span class="ps-4">$100</span>
            </li>
          </ul>
        </div>

        <!-- Active orders -->
        <div class="ms-5 mt-5 w-75">

          <div>
            <h2>Welcome, <span>username</span></h2>
            <p>Find important booking here</p>
          </div>
          <div
            id="navbar-example2"
            class="navbar px-3 mb-3"
            style="background-color: #fff"
          >
            <a class="navbar-brand" href="#">Active Bookings</a>
            <ul class="nav nav-pills">
              <li class="nav-item dropdown">
                <a
                  class="nav-link dropdown-toggle"
                  data-bs-toggle="dropdown"
                  href="#"
                  role="button"
                  aria-expanded="false"
                  >Active Bookings (1)</a
                >
                <ul class="dropdown-menu">
                  <li>
                    <a class="dropdown-item" href="#scrollspyHeading3"
                      >Booking 1</a
                    >
                  </li>
                </ul>
              </li>
            </ul>
          </div>
        </div>
      </div>

